{
  "extends": [
    "airbnb-base",
    "plugin:vue/vue3-recommended"
  ],
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  },
  "rules": {
    "func-names": 0,
    "prefer-destructuring": 0,
    "no-param-reassign": ["error", { "props": false }],
    "no-console": ["error", { "allow": ["warn", "error"] }],
    "vue/max-attributes-per-line": ["error", {
      "singleline": 2,      
      "multiline": 1
    }]
  }
}
